<h6>This is DashBoard</h6>
<h3>Welcome {{name}}</h3>
<p>Here are your stories </p>
{{#if stories}}
 <table class="stripped">
     <thead>
         <tr>
             <th>Title</th>
             <th>Date</th>
             <th>Status</th>
             <th>Action</th>
         </tr>
     </thead>
     <tbody>
         {{#each stories}}
           <tr>
               <td>
                   {{!-- it is smart enough to get id form story object as below --}}
                   <a href="/stories/{{_id}}">{{title}}</a> 
               </td>
                 <td>
                   {{formatDate createdAt 'MMMM Do YYYY, h:mm:ss a'}}
               </td>
               <td>
                   <span class="dash-status">{{status}}</span>
               </td>
               <td>
                   
               </td>
           </tr>
         {{/each}}
     </tbody>
 </table>

{{else}}
<p>You have not created any stories</p>

{{/if}}